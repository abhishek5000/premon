	rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl> ;
	rdfs:label "denotes"@en ;
	rdfs:range semiotics:Reference ;
	rdfs:domain semiotics:InformationEntity ;
	owl:propertyChainAxiom _:node1advgngtrx351 .

