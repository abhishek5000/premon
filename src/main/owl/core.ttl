###  http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#annotation

nif:annotation rdf:type owl:ObjectProperty .



###  http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#referenceContext

nif:referenceContext rdf:type owl:ObjectProperty .



###  http://premon.fbk.eu/ns/premon#argumentRel

:argumentRel rdf:type owl:ObjectProperty ;

             rdfs:label "semantic argument relation"@en ;

             rdfs:range :SemanticArgument ;

             rdfs:domain :SemanticArgument .



###  http://premon.fbk.eu/ns/premon#evokedConcept

:evokedConcept rdf:type owl:FunctionalProperty ,
                        owl:ObjectProperty ;

               rdfs:domain :Conceptualization ;

               rdfs:subPropertyOf skos:broader ;

               rdfs:range ontolex:LexicalConcept .



###  http://premon.fbk.eu/ns/premon#evokingEntry

:evokingEntry rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;

              rdfs:label "meaning of"@en ;

              rdfs:domain :Conceptualization ;

              rdfs:range ontolex:LexicalEntry .



###  http://premon.fbk.eu/ns/premon#predicateRel

:predicateRel rdf:type owl:ObjectProperty ;

              rdfs:label "semantic frame relation"@en ;

              rdfs:domain :Predicate ;

              rdfs:range :Predicate .



###  http://premon.fbk.eu/ns/premon#role

:role rdf:type owl:FunctionalProperty ,
               owl:ObjectProperty ;

      rdfs:label "role"@en ;

      rdfs:domain :SemanticArgument ;

      rdfs:range :SemanticRole .



###  http://premon.fbk.eu/ns/premon#semArg

:semArg rdf:type owl:ObjectProperty ;

        rdfs:label "semantic argument"@en ;

        rdfs:domain :Predicate ;

        rdfs:range :SemanticArgument .



###  http://premon.fbk.eu/ns/premon#semType

:semType rdf:type owl:ObjectProperty ;

         rdfs:label "semantic type"@en ;

         rdfs:range :SemanticType ;

         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Predicate
                                     :SemanticArgument
                                   )
                     ] .



###  http://premon.fbk.eu/ns/premon#typeRel

:typeRel rdf:type owl:ObjectProperty ;

         rdfs:label "semantic type relation"@en ;

         rdfs:domain :SemanticType ;

         rdfs:range :SemanticType .



###  http://www.w3.org/2004/02/skos/core#broader

skos:broader rdf:type owl:ObjectProperty .



###  http://www.w3.org/ns/lemon/ontolex#evokes

ontolex:evokes owl:propertyChainAxiom ( [ owl:inverseOf :evokingEntry
                                        ]
                                        :evokedConcept
                                      ) .





#################################################################
#
#    Classes
