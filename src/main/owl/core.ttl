#################################################################


###  http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#Context

nif:Context rdf:type owl:Class .



###  http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#RFC5147String

nif:RFC5147String rdf:type owl:Class .



###  http://premon.fbk.eu/ns/premon#Conceptualization

:Conceptualization rdf:type owl:Class ;

                   rdfs:label "entry concept"@en ;

                   rdfs:subClassOf skos:Concept ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :evokingEntry ;
                                     owl:onClass ontolex:LexicalEntry ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :evokedConcept ;
                                     owl:onClass ontolex:LexicalConcept ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                   ] .



###  http://premon.fbk.eu/ns/premon#Example

:Example rdf:type owl:Class ;

         rdfs:label "example" ;

         rdfs:subClassOf nif:Context .



###  http://premon.fbk.eu/ns/premon#Markable

:Markable rdf:type owl:Class ;

          rdfs:label "markable" ;

          rdfs:subClassOf nif:RFC5147String ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty nif:referenceContext ;
                            owl:onClass :Example ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty nif:referenceContext ;
                            owl:allValuesFrom :Example
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty nif:annotation ;
                            owl:someValuesFrom [ rdf:type owl:Class ;
                                                 owl:unionOf ( :Predicate
                                                               :SemanticArgument
                                                             )
                                               ]
                          ] .



###  http://premon.fbk.eu/ns/premon#Predicate

:Predicate rdf:type owl:Class ;

           rdfs:label "predicate"@en ;

           rdfs:subClassOf ontolex:LexicalConcept .



###  http://premon.fbk.eu/ns/premon#SemanticArgument

:SemanticArgument rdf:type owl:Class ;

                  rdfs:label "semantic argument"@en ;

                  rdfs:subClassOf ontolex:LexicalConcept .



###  http://premon.fbk.eu/ns/premon#SemanticRole

:SemanticRole rdf:type owl:Class ;

              rdfs:label "semantic role"@en .



###  http://premon.fbk.eu/ns/premon#SemanticType

:SemanticType rdf:type owl:Class ;

              rdfs:label "semantic type"@en ;

              rdfs:subClassOf ontolex:LexicalConcept .




