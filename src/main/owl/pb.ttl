###  http://premon.fbk.eu/ns/premon-pb#aspect

:aspect rdf:type owl:ObjectProperty ;

        rdfs:label "aspect" ;

        rdfs:range :Aspect ;

        rdfs:domain :Inflection .



###  http://premon.fbk.eu/ns/premon-pb#form

:form rdf:type owl:FunctionalProperty ,
               owl:ObjectProperty ;

      rdfs:label "form" ;

      rdfs:range :Form ;

      rdfs:domain :Inflection .



###  http://premon.fbk.eu/ns/premon-pb#functionTag

:functionTag rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;

             rdfs:label "function tag" ;

             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :ModifierRole
                                        :Preposition
                                        :Tag
                                      )
                        ] .



###  http://premon.fbk.eu/ns/premon-pb#inflection

:inflection rdf:type owl:ObjectProperty ;

            rdfs:label "inflection" ;

            rdfs:range :Inflection .



###  http://premon.fbk.eu/ns/premon-pb#person

:person rdf:type owl:FunctionalProperty ,
                 owl:ObjectProperty ;

        rdfs:label "person" ;

        rdfs:domain :Inflection ;

        rdfs:range :Person .



###  http://premon.fbk.eu/ns/premon-pb#tense

:tense rdf:type owl:FunctionalProperty ,
                owl:ObjectProperty ;

       rdfs:label "tense" ;

       rdfs:domain :Inflection ;

       rdfs:range :Tense .



###  http://premon.fbk.eu/ns/premon-pb#voice

:voice rdf:type owl:FunctionalProperty ,
