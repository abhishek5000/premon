#################################################################
#
#    Annotation properties
#
#################################################################


###  http://www.w3.org/2004/02/skos/core#broader

skos:broader rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2004/02/skos/core#closeMatch

skos:closeMatch rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2004/02/skos/core#related

skos:related rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://premon.fbk.eu/ontology/vn#first

pmovn:first rdf:type owl:ObjectProperty ;
            
            rdfs:subPropertyOf pmovn:item .



###  http://premon.fbk.eu/ontology/vn#frame

pmovn:frame rdf:type owl:ObjectProperty ;
            
            rdfs:domain pmovn:VerbClass ;
            
            rdfs:range pmovn:VerbFrame .



###  http://premon.fbk.eu/ontology/vn#item

pmovn:item rdf:type owl:ObjectProperty ;
           
           owl:propertyChainAxiom ( pmovn:first
                                    pmovn:next
                                  ) .



###  http://premon.fbk.eu/ontology/vn#itemValue

pmovn:itemValue rdf:type owl:ObjectProperty ;
                
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( pmovn:Arg
                                            pmovn:Pred
                                            pmovn:SynItem
                                          )
                            ] .



###  http://premon.fbk.eu/ontology/vn#next

pmovn:next rdf:type owl:ObjectProperty .



###  http://premon.fbk.eu/ontology/vn#property

pmovn:property rdf:type owl:ObjectProperty ;
               
               rdfs:subPropertyOf owl:topObjectProperty ;
               
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( pmovn:AtomicSelectionalRestriction
                                           pmovn:AtomicSyntacticRestriction
                                         )
                           ] ;
               
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( pmovn:PrepositionalRestrictionProperty
                                          pmovn:RoleRestrictionProperty
                                          pmovn:SyntacticRestrictionProperty
                                        )
                          ] .



###  http://premon.fbk.eu/ontology/vn#restrictionItem

pmovn:restrictionItem rdf:type owl:ObjectProperty ;
                      
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( pmovn:SelectionalRestriction
                                                 pmovn:SyntacticRestriction
                                               )
                                 ] ;
                      
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( pmovn:SelectionalRestrictionSet
                                                  pmovn:SyntacticRestrictionSet
                                                )
                                  ] .



###  http://premon.fbk.eu/ontology/vn#selRestriction

pmovn:selRestriction rdf:type owl:ObjectProperty ;
                     
                     rdfs:range pmovn:SelectionalRestrictionSet ;
                     
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( pmovn:NpSynItem
                                                 pmovn:PrepSynItem
                                                 pmovn:SemanticArgument
                                               )
                                 ] .



###  http://premon.fbk.eu/ontology/vn#synRestriction

pmovn:synRestriction rdf:type owl:ObjectProperty ;
                     
                     rdfs:domain pmovn:NpSynItem ;
                     
                     rdfs:range pmovn:SyntacticRestrictionSet ;
                     
                     rdfs:subPropertyOf owl:topObjectProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://premon.fbk.eu/ontology/vn#frameDescNumber

pmovn:frameDescNumber rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      
                      rdfs:domain pmovn:VerbFrame ;
                      
                      rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#framePrimary

pmovn:framePrimary rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   
                   rdfs:domain pmovn:VerbFrame ;
                   
                   rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#frameSecondary

pmovn:frameSecondary rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     
                     rdfs:domain pmovn:VerbFrame ;
                     
                     rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#frameSemanticsDescription

pmovn:frameSemanticsDescription rdf:type owl:DatatypeProperty ;
                                
                                rdfs:label "String representing the semantics of the Frame" ;
                                
                                rdfs:domain pmovn:VerbFrame ;
                                
                                rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#frameSyntaxDescription

pmovn:frameSyntaxDescription rdf:type owl:DatatypeProperty ;
                             
                             rdfs:label "String representing the syntax of the Frame" ;
                             
                             rdfs:domain pmovn:VerbFrame ;
                             
                             rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#frameXtag

pmovn:frameXtag rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                
                rdfs:domain pmovn:VerbFrame ;
                
                rdfs:range xsd:string .





#################################################################
#
#    Classes
#
#################################################################


###  http://premon.fbk.eu/ontology/core#SemanticArgument

pmo:SemanticArgument rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty pmovn:selRestriction ;
                                       owl:allValuesFrom pmovn:SelectionalRestriction
                                     ] .



###  http://premon.fbk.eu/ontology/vn#AbsentAtomicSyntacticRestriction

pmovn:AbsentAtomicSyntacticRestriction rdf:type owl:Class ;
                                       
                                       rdfs:subClassOf pmovn:AtomicSelectionalRestriction ,
                                                       pmovn:AtomicSyntacticRestriction .



###  http://premon.fbk.eu/ontology/vn#AdjSynItem

pmovn:AdjSynItem rdf:type owl:Class ;
                 
                 rdfs:subClassOf pmovn:SynItem ;
                 
                 skos:closeMatch <http://www.lexinfo.net/lexinfo#Adjective> .



###  http://premon.fbk.eu/ontology/vn#AdvSynItem

pmovn:AdvSynItem rdf:type owl:Class ;
                 
                 rdfs:subClassOf pmovn:SynItem ;
                 
                 skos:closeMatch <http://www.lexinfo.net/lexinfo#Adverb> .



###  http://premon.fbk.eu/ontology/vn#AndSelectionalRestrictionSet

pmovn:AndSelectionalRestrictionSet rdf:type owl:Class ;
                                   
                                   rdfs:subClassOf pmovn:SelectionalRestrictionSet ;
                                   
                                   rdfs:comment "default" .



###  http://premon.fbk.eu/ontology/vn#AndSyntacticRestrictionSet

pmovn:AndSyntacticRestrictionSet rdf:type owl:Class ;
                                 
                                 rdfs:subClassOf pmovn:SyntacticRestrictionSet ;
                                 
                                 rdfs:comment "default" .



###  http://premon.fbk.eu/ontology/vn#Arg

pmovn:Arg rdf:type owl:Class ;
          
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( pmovn:ConstantArg
                                              pmovn:EventArg
                                              pmovn:ThemRoleArg
                                              pmovn:VerbSpecificArg
                                            )
                              ] .



###  http://premon.fbk.eu/ontology/vn#AtomicSelectionalRestriction

pmovn:AtomicSelectionalRestriction rdf:type owl:Class ;
                                   
                                   owl:equivalentClass [ rdf:type owl:Class ;
                                                         owl:unionOf ( pmovn:AbsentAtomicSyntacticRestriction
                                                                       pmovn:ExistAtomicSelectionalRestriction
                                                                     )
                                                       ] ;
                                   
                                   rdfs:subClassOf pmovn:SelectionalRestriction .



###  http://premon.fbk.eu/ontology/vn#AtomicSyntacticRestriction

pmovn:AtomicSyntacticRestriction rdf:type owl:Class ;
                                 
                                 owl:equivalentClass [ rdf:type owl:Class ;
                                                       owl:unionOf ( pmovn:AbsentAtomicSyntacticRestriction
                                                                     pmovn:ExistAtomicSyntacticRestriction
                                                                   )
                                                     ] ;
                                 
                                 rdfs:subClassOf pmovn:SyntacticRestriction .



###  http://premon.fbk.eu/ontology/vn#AuxnpType

pmovn:AuxnpType rdf:type owl:Class .



###  http://premon.fbk.eu/ontology/vn#ConstantArg

pmovn:ConstantArg rdf:type owl:Class ;
                  
                  rdfs:subClassOf pmovn:Arg .



###  http://premon.fbk.eu/ontology/vn#EventArg

pmovn:EventArg rdf:type owl:Class ;
               
               rdfs:subClassOf pmovn:Arg .



###  http://premon.fbk.eu/ontology/vn#EventArgType

pmovn:EventArgType rdf:type owl:Class ;
                   
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( pmovn:startEventArg
                                                     pmovn:resultEventArg
                                                     pmovn:endEventArg
                                                     pmovn:duringEventArg
                                                   )
                                       ] .



###  http://premon.fbk.eu/ontology/vn#Example

pmovn:Example rdf:type owl:Class ;
              
              rdfs:subClassOf pmo:Example .



###  http://premon.fbk.eu/ontology/vn#ExistAtomicSelectionalRestriction

pmovn:ExistAtomicSelectionalRestriction rdf:type owl:Class ;
                                        
                                        rdfs:subClassOf pmovn:AtomicSelectionalRestriction .



###  http://premon.fbk.eu/ontology/vn#ExistAtomicSyntacticRestriction

pmovn:ExistAtomicSyntacticRestriction rdf:type owl:Class ;
                                      
                                      rdfs:subClassOf pmovn:AtomicSyntacticRestriction .



###  http://premon.fbk.eu/ontology/vn#ImplicitArg

pmovn:ImplicitArg rdf:type owl:Class ;
                  
                  rdfs:subClassOf pmovn:Arg .



###  http://premon.fbk.eu/ontology/vn#LexSynItem

pmovn:LexSynItem rdf:type owl:Class ;
                 
                 rdfs:subClassOf pmovn:SynItem ;
                 
                 rdfs:comment "Not clear if there is an equivalent in lexInfo" .



###  http://premon.fbk.eu/ontology/vn#NegPred

pmovn:NegPred rdf:type owl:Class ;
              
              rdfs:subClassOf pmovn:Pred .



###  http://premon.fbk.eu/ontology/vn#NpSynItem

pmovn:NpSynItem rdf:type owl:Class ;
                
                rdfs:subClassOf pmovn:SynItem ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pmovn:itemValue ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:unionOf ( pmovn:AuxnpType
                                                                    pmovn:ThematicRole
                                                                  )
                                                    ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pmovn:selRestriction ;
                                  owl:allValuesFrom pmovn:SelectionalRestriction
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pmovn:synRestriction ;
                                  owl:allValuesFrom pmovn:SyntacticRestriction
                                ] ;
                
                skos:closeMatch <http://www.lexinfo.net/lexinfo#NounPhrase> .



###  http://premon.fbk.eu/ontology/vn#OrSelectionalRestrictionSet

pmovn:OrSelectionalRestrictionSet rdf:type owl:Class ;
                                  
                                  rdfs:subClassOf pmovn:SelectionalRestrictionSet .



###  http://premon.fbk.eu/ontology/vn#OrSyntacticRestrictionSet

pmovn:OrSyntacticRestrictionSet rdf:type owl:Class ;
                                
                                rdfs:subClassOf pmovn:SyntacticRestrictionSet .



###  http://premon.fbk.eu/ontology/vn#Pred

pmovn:Pred rdf:type owl:Class ;
           
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty pmovn:itemValue ;
                             owl:allValuesFrom pmovn:PredType
                           ] .



