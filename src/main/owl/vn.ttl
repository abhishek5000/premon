###  http://premon.fbk.eu/ontology/vn#SelectionalRestriction

pmovn:SelectionalRestriction rdf:type owl:Class ;
                             
                             owl:equivalentClass [ rdf:type owl:Class ;
                                                   owl:unionOf ( pmovn:PrepositionalRestrictionProperty
                                                                 pmovn:RoleRestrictionProperty
                                                                 pmovn:SelectionalRestrictionSet
                                                               )
                                                 ] .



###  http://premon.fbk.eu/ontology/vn#SelectionalRestrictionProperty

pmovn:SelectionalRestrictionProperty rdf:type owl:Class ;
                                     
                                     owl:equivalentClass [ rdf:type owl:Class ;
                                                           owl:unionOf ( pmovn:PrepositionalRestrictionProperty
                                                                         pmovn:RoleRestrictionProperty
                                                                       )
                                                         ] .



###  http://premon.fbk.eu/ontology/vn#SelectionalRestrictionSet

pmovn:SelectionalRestrictionSet rdf:type owl:Class ;
                                
                                owl:equivalentClass [ rdf:type owl:Class ;
                                                      owl:unionOf ( pmovn:AndSelectionalRestrictionSet
                                                                    pmovn:OrSelectionalRestrictionSet
                                                                  )
                                                    ] ;
                                
                                rdfs:subClassOf pmovn:SelectionalRestriction ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty pmovn:restrictionItem ;
                                                  owl:someValuesFrom pmovn:SelectionalRestriction
                                                ] .



###  http://premon.fbk.eu/ontology/vn#SemanticArgument

pmovn:SemanticArgument rdf:type owl:Class ;
                       
