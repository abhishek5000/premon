@prefix : <http://premon.fbk.eu/ontology/all#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix pm: <http://premon.fbk.eu/resource/> .
@prefix nif: <http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pmo: <http://premon.fbk.eu/ontology/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix lime: <http://www.w3.org/ns/lemon/lime#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix pmovn: <http://premon.fbk.eu/ontology/vn#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix ontolex: <http://www.w3.org/ns/lemon/ontolex#> .
@base <http://premon.fbk.eu/ontology/vn> .

<http://premon.fbk.eu/ontology/vn> rdf:type owl:Ontology ;
                                   
                                   dc:creator "Marco Rospocher <https://dkm.fbk.eu/people/profile/rospocher>" ;
                                   
                                   vann:preferredNamespaceUri "http://premon.fbk.eu/ontology/vn#" ;
                                   
                                   rdfs:comment "The VerbNet module of the PreMOn ontology extends the core module for representing concepts specific to VerbNet." ;
                                   
                                   dc:publisher "DKM, Fondazione Bruno Kessler <http://dkm.fbk.eu/>" ;
                                   
                                   vann:preferredNamespacePrefix "pmovn" ;
                                   
                                   dc:creator "Francesco Corcoglioniti" ;
                                   
                                   dcterms:title "Predicate Model for Ontologies (PreMOn) - VerbNet ontology module"@en ;
                                   
                                   dc:description """The VerbNet module of the PreMOn ontology extends the core module with classes and properties specific to the VerbNet predicate model. The modelling is based on the specification at <http://nlp.cs.nyu.edu/meyers/nombank/nombank-specs-2007.pdf>.

Subclasses of Predicate, SemanticArgument and SemanticRole of the core module are defined for representing elements specific to VerbNet."""@en ;
                                   
                                   dc:rights "This ontology is licensed under CC-BY <http://creativecommons.org/licenses/by/4.0/>"@en ;
                                   
                                   dcterms:license <http://creativecommons.org/licenses/by/4.0/> ;
                                   
                                   dc:description <http://premon.fbk.eu/images/vn.svg> ;
                                   
                                   owl:imports <http://premon.fbk.eu/ontology/core> ;
                                   
                                   rdfs:isDefinedBy <http://premon.fbk.eu/ontology/vn.ttl> ;
                                   
                                   owl:imports <http://www.w3.org/ns/lemon/ontolex> .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://www.w3.org/2004/02/skos/core#broader

skos:broader rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2004/02/skos/core#closeMatch

skos:closeMatch rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2004/02/skos/core#related

skos:related rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://premon.fbk.eu/ontology/vn#definesFrame

pmovn:definesFrame rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "defines frame"@en ;
                   
                   rdfs:subPropertyOf pmovn:frame .



###  http://premon.fbk.eu/ontology/vn#definesSemRole

pmovn:definesSemRole rdf:type owl:ObjectProperty ;
                     
                     rdfs:subPropertyOf pmo:semRole ;
                     
                     rdfs:range pmovn:SemanticRole ;
                     
                     rdfs:domain pmovn:VerbClass .



###  http://premon.fbk.eu/ontology/vn#frame

pmovn:frame rdf:type owl:ObjectProperty ;
            
            rdfs:label "frame"@en ;
            
            rdfs:domain pmovn:VerbClass ;
            
            rdfs:range pmovn:VerbNetFrame .



###  http://premon.fbk.eu/ontology/vn#restriction

pmovn:restriction rdf:type owl:ObjectProperty ;
                  
                  rdfs:range pmovn:Restriction ;
                  
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( pmovn:NpSynItem
                                              pmovn:PrepSynItem
                                              pmovn:SemanticRole
                                            )
                              ] .



###  http://premon.fbk.eu/ontology/vn#subclassOf

pmovn:subclassOf rdf:type owl:ObjectProperty ;
                 
                 rdfs:subPropertyOf pmo:classRel ;
                 
                 rdfs:domain pmovn:VerbClass ;
                 
                 rdfs:range pmovn:VerbClass ;
                 
                 rdfs:subPropertyOf skos:broader .



###  http://premon.fbk.eu/ontology/vn#thematicRole

pmovn:thematicRole rdf:type owl:FunctionalProperty ,
                            owl:ObjectProperty ;
                   
                   rdfs:domain pmovn:SemanticRole ;
                   
                   rdfs:range pmovn:ThematicRole .



###  http://www.lexinfo.net/ontology/2.0/lexinfo#partOfSpeech

lexinfo:partOfSpeech rdf:type owl:ObjectProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://premon.fbk.eu/ontology/vn#frameDescNumber

pmovn:frameDescNumber rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      
                      rdfs:domain pmovn:VerbNetFrame ;
                      
                      rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#framePrimary

pmovn:framePrimary rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   
                   rdfs:domain pmovn:VerbNetFrame ;
                   
                   rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#frameSecondary

pmovn:frameSecondary rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     
                     rdfs:domain pmovn:VerbNetFrame ;
                     
                     rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#frameSemanticsDescription

pmovn:frameSemanticsDescription rdf:type owl:DatatypeProperty ;
                                
                                rdfs:label "String representing the semantics of the Frame" ;
                                
                                rdfs:domain pmovn:VerbNetFrame ;
                                
                                rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#frameSyntaxDescription

pmovn:frameSyntaxDescription rdf:type owl:DatatypeProperty ;
                             
                             rdfs:label "String representing the syntax of the Frame" ;
                             
                             rdfs:domain pmovn:VerbNetFrame ;
                             
                             rdfs:range xsd:string .



###  http://premon.fbk.eu/ontology/vn#frameXtag

pmovn:frameXtag rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                
                rdfs:domain pmovn:VerbNetFrame ;
                
                rdfs:range xsd:string .





#################################################################
#
#    Classes
#
#################################################################


###  http://premon.fbk.eu/ontology/vn#AbsentAtomicRestriction

pmovn:AbsentAtomicRestriction rdf:type owl:Class ;
                              
                              rdfs:subClassOf pmovn:AtomicRestriction .



###  http://premon.fbk.eu/ontology/vn#AdjSynItem

pmovn:AdjSynItem rdf:type owl:Class ;
                 
                 rdfs:subClassOf pmovn:SynItem ;
                 
                 skos:closeMatch <http://www.lexinfo.net/lexinfo#Adjective> .



###  http://premon.fbk.eu/ontology/vn#AdvSynItem

pmovn:AdvSynItem rdf:type owl:Class ;
                 
                 rdfs:subClassOf pmovn:SynItem ;
                 
                 skos:closeMatch <http://www.lexinfo.net/lexinfo#Adverb> .



###  http://premon.fbk.eu/ontology/vn#AndCompoundRestriction

pmovn:AndCompoundRestriction rdf:type owl:Class ;
                             
                             rdfs:subClassOf pmovn:CompoundRestriction ;
                             
                             rdfs:comment "default" .



###  http://premon.fbk.eu/ontology/vn#AtomicRestriction

pmovn:AtomicRestriction rdf:type owl:Class ;
                        
                        rdfs:subClassOf pmovn:Restriction ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty pmo:valueObj ;
                                          owl:cardinality "1"^^xsd:nonNegativeInteger
                                        ] ;
                        
                        owl:disjointUnionOf ( pmovn:AbsentAtomicRestriction
                                              pmovn:ExistAtomicRestriction
                                            ) .



###  http://premon.fbk.eu/ontology/vn#AuxnpType

pmovn:AuxnpType rdf:type owl:Class ;
                
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:oneOf ( pmovn:oblique_auxnpType
                                                  pmovn:oblique2_auxnpType
                                                  pmovn:oblique1_auxnpType
                                                  pmovn:np_auxnpType
                                                )
                                    ] .



###  http://premon.fbk.eu/ontology/vn#CompoundRestriction

pmovn:CompoundRestriction rdf:type owl:Class ;
                          
                          rdfs:subClassOf pmovn:Restriction ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty pmo:item ;
                                            owl:someValuesFrom owl:Thing
                                          ] ;
                          
                          owl:disjointUnionOf ( pmovn:AndCompoundRestriction
                                                pmovn:OrCompoundRestriction
                                              ) .



###  http://premon.fbk.eu/ontology/vn#ConstantPredArg

pmovn:ConstantPredArg rdf:type owl:Class ;
                      
                      rdfs:subClassOf pmovn:PredArg .



###  http://premon.fbk.eu/ontology/vn#EventPredArg

pmovn:EventPredArg rdf:type owl:Class ;
                   
                   rdfs:subClassOf pmovn:PredArg ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty pmo:valueObj ;
                                     owl:cardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty pmo:valueObj ;
                                     owl:allValuesFrom pmovn:EventPredArgType
                                   ] .



###  http://premon.fbk.eu/ontology/vn#EventPredArgType

pmovn:EventPredArgType rdf:type owl:Class ;
                       
                       owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:oneOf ( pmovn:startEventArg
                                                         pmovn:resultEventArg
                                                         pmovn:endEventArg
                                                         pmovn:duringEventArg
                                                       )
                                           ] .



###  http://premon.fbk.eu/ontology/vn#ExistAtomicRestriction

pmovn:ExistAtomicRestriction rdf:type owl:Class ;
                             
                             rdfs:subClassOf pmovn:AtomicRestriction .



###  http://premon.fbk.eu/ontology/vn#ImplicitPredArg

pmovn:ImplicitPredArg rdf:type owl:Class ;
                      
                      rdfs:subClassOf pmovn:PredArg .



###  http://premon.fbk.eu/ontology/vn#LexSynItem

pmovn:LexSynItem rdf:type owl:Class ;
                 
                 rdfs:subClassOf pmovn:SynItem ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty pmo:valueDt ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange xsd:string
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty pmo:valueDt ;
                                   owl:allValuesFrom xsd:string
                                 ] ;
                 
                 rdfs:comment "Not clear if there is an equivalent in lexInfo" .



###  http://premon.fbk.eu/ontology/vn#NegPred

pmovn:NegPred rdf:type owl:Class ;
              
              rdfs:subClassOf pmovn:Pred .



###  http://premon.fbk.eu/ontology/vn#NpSynItem

pmovn:NpSynItem rdf:type owl:Class ;
                
                rdfs:subClassOf pmovn:SynItem ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pmovn:restriction ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:unionOf ( pmovn:RoleSelectionalRestriction
                                                                    pmovn:SyntacticRestriction
                                                                  )
                                                    ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pmo:valueObj ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:unionOf ( pmovn:AuxnpType
                                                                    pmovn:SemanticRole
                                                                  )
                                                    ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pmo:valueObj ;
                                  owl:cardinality "1"^^xsd:nonNegativeInteger
                                ] ;
                
                skos:closeMatch <http://www.lexinfo.net/lexinfo#NounPhrase> .



###  http://premon.fbk.eu/ontology/vn#OrCompoundRestriction

pmovn:OrCompoundRestriction rdf:type owl:Class ;
                            
                            rdfs:subClassOf pmovn:CompoundRestriction .



###  http://premon.fbk.eu/ontology/vn#Pred

pmovn:Pred rdf:type owl:Class ;
           
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty pmo:item ;
                             owl:someValuesFrom pmovn:PredArg
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty pmo:valueObj ;
                             owl:allValuesFrom pmovn:PredType
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty pmo:first ;
                             owl:onClass pmovn:PredArg ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty pmo:valueObj ;
                             owl:someValuesFrom owl:Thing
                           ] .



###  http://premon.fbk.eu/ontology/vn#PredArg

pmovn:PredArg rdf:type owl:Class ;
              
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty pmo:valueDt ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty pmo:valueDt ;
                                owl:allValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty pmo:next ;
                                owl:allValuesFrom pmovn:PredArg
                              ] ;
              
              owl:disjointUnionOf ( pmovn:ConstantPredArg
                                    pmovn:EventPredArg
                                    pmovn:ThemRolePredArg
                                    pmovn:VerbSpecificPredArg
                                  ) .



###  http://premon.fbk.eu/ontology/vn#PredType

pmovn:PredType rdf:type owl:Class .



###  http://premon.fbk.eu/ontology/vn#PrepSynItem

pmovn:PrepSynItem rdf:type owl:Class ;
                  
                  rdfs:subClassOf pmovn:SynItem ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty pmo:valueObj ;
                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                        owl:intersectionOf ( ontolex:LexicalEntry
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty lexinfo:partOfSpeech ;
                                                                               owl:hasValue lexinfo:preposition
                                                                             ]
                                                                           )
                                                      ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty pmovn:restriction ;
                                    owl:allValuesFrom pmovn:PrepositionSelectionalRestriction
                                  ] ,
                                  [ rdf:type owl:Class ;
                                    owl:unionOf ( [ rdf:type owl:Class ;
                                                    owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                           owl:complementOf [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty pmo:valueObj ;
                                                                                              owl:someValuesFrom owl:Thing
                                                                                            ]
                                                                         ]
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty pmovn:restriction ;
                                                                           owl:someValuesFrom owl:Thing
                                                                         ]
                                                                       )
                                                  ]
                                                  [ rdf:type owl:Class ;
                                                    owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                           owl:complementOf [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty pmovn:restriction ;
                                                                                              owl:someValuesFrom owl:Thing
                                                                                            ]
                                                                         ]
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty pmo:valueObj ;
                                                                           owl:someValuesFrom owl:Thing
                                                                         ]
                                                                       )
                                                  ]
                                                )
                                  ] ;
                  
                  skos:closeMatch <http://www.lexinfo.net/lexinfo#Preposition> .



###  http://premon.fbk.eu/ontology/vn#PrepositionRestrictionProperty

pmovn:PrepositionRestrictionProperty rdf:type owl:Class ;
                                     
                                     owl:equivalentClass [ rdf:type owl:Class ;
                                                           owl:oneOf ( pmovn:dest_prp
                                                                       pmovn:path_prp
                                                                       pmovn:dir_prp
                                                                       pmovn:loc_prp
                                                                       pmovn:spatial_prp
                                                                       pmovn:plural_prp
                                                                       pmovn:src_prp
                                                                       pmovn:dest_dir_prp
                                                                       pmovn:dest_conf_prp
                                                                     )
                                                         ] ;
                                     
                                     rdfs:subClassOf pmovn:SelectionalRestrictionProperty .



###  http://premon.fbk.eu/ontology/vn#PrepositionSelectionalRestriction

pmovn:PrepositionSelectionalRestriction rdf:type owl:Class ;
                                        
                                        rdfs:subClassOf pmovn:SelectionalRestriction ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty pmo:valueObj ;
                                                          owl:allValuesFrom pmovn:PrepositionRestrictionProperty
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty pmo:item ;
                                                          owl:allValuesFrom pmovn:PrepositionSelectionalRestriction
                                                        ] .



###  http://premon.fbk.eu/ontology/vn#Restriction

pmovn:Restriction rdf:type owl:Class ;
                  
                  owl:disjointUnionOf ( pmovn:SelectionalRestriction
                                        pmovn:SyntacticRestriction
                                      ) ,
                                      ( pmovn:AtomicRestriction
                                        pmovn:CompoundRestriction
                                      ) .



###  http://premon.fbk.eu/ontology/vn#RestrictionProperty

pmovn:RestrictionProperty rdf:type owl:Class .



###  http://premon.fbk.eu/ontology/vn#RoleRestrictionProperty

pmovn:RoleRestrictionProperty rdf:type owl:Class ;
                              
                              owl:equivalentClass [ rdf:type owl:Class ;
                                                    owl:oneOf ( pmovn:machine_rrp
                                                                pmovn:pointy_rrp
                                                                pmovn:animate_rrp
                                                                pmovn:comestible_rrp
                                                                pmovn:scalar_rrp
                                                                pmovn:shape_rrp
                                                                pmovn:region_rrp
                                                                pmovn:communication_rrp
                                                                pmovn:plant_rrp
                                                                pmovn:body_part_rrp
                                                                pmovn:nonrigid_rrp
                                                                pmovn:human_rrp
                                                                pmovn:int_control_rrp
                                                                pmovn:substance_rrp
                                                                pmovn:organization_rrp
                                                                pmovn:concrete_rrp
                                                                pmovn:biotic_rrp
                                                                pmovn:tool_rrp
                                                                pmovn:rigid_rrp
                                                                pmovn:sound_rrp
                                                                pmovn:vehicle_rrp
                                                                pmovn:solid_rrp
                                                                pmovn:artifact_rrp
                                                                pmovn:force_rrp
                                                                pmovn:location_rrp
                                                                pmovn:time_rrp
                                                                pmovn:animal_rrp
                                                                pmovn:elongated_rrp
                                                                pmovn:phys_obj_rrp
                                                                pmovn:abstract_rrp
                                                                pmovn:state_rrp
                                                                pmovn:currency_rrp
                                                                pmovn:garment_rrp
                                                                pmovn:refl_rrp
                                                              )
                                                  ] ;
                              
                              rdfs:subClassOf pmovn:SelectionalRestrictionProperty .



###  http://premon.fbk.eu/ontology/vn#RoleSelectionalRestriction

pmovn:RoleSelectionalRestriction rdf:type owl:Class ;
                                 
                                 rdfs:subClassOf pmovn:SelectionalRestriction ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty pmo:item ;
                                                   owl:allValuesFrom pmovn:RoleSelectionalRestriction
                                                 ] ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty pmo:valueObj ;
                                                   owl:allValuesFrom pmovn:RoleRestrictionProperty
                                                 ] .



###  http://premon.fbk.eu/ontology/vn#SelectionalRestriction

pmovn:SelectionalRestriction rdf:type owl:Class ;
                             
                             rdfs:subClassOf pmovn:Restriction ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty pmo:valueObj ;
                                               owl:allValuesFrom pmovn:SelectionalRestrictionProperty
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty pmo:item ;
                                               owl:allValuesFrom pmovn:SelectionalRestriction
                                             ] ;
                             
                             owl:disjointUnionOf ( pmovn:PrepositionSelectionalRestriction
                                                   pmovn:RoleSelectionalRestriction
                                                 ) .



###  http://premon.fbk.eu/ontology/vn#SelectionalRestrictionProperty

pmovn:SelectionalRestrictionProperty rdf:type owl:Class ;
                                     
                                     rdfs:subClassOf pmovn:RestrictionProperty ;
                                     
                                     owl:disjointUnionOf ( pmovn:PrepositionRestrictionProperty
                                                           pmovn:RoleRestrictionProperty
                                                         ) .



###  http://premon.fbk.eu/ontology/vn#SemanticRole

pmovn:SemanticRole rdf:type owl:Class ;
                   
                   rdfs:label "VerbNet semantic argument"@en ;
                   
                   rdfs:subClassOf pmo:SemanticRole ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty [ owl:inverseOf pmo:semRole
                                                    ] ;
                                     owl:allValuesFrom pmovn:VerbClass
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty pmovn:thematicRole ;
                                     owl:someValuesFrom pmovn:ThematicRole
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty pmovn:restriction ;
                                     owl:allValuesFrom pmovn:RoleRestrictionProperty
                                   ] .



###  http://premon.fbk.eu/ontology/vn#SynItem

pmovn:SynItem rdf:type owl:Class ;
              
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty pmo:next ;
                                owl:allValuesFrom pmovn:SynItem
                              ] ;
              
              owl:disjointUnionOf ( pmovn:AdjSynItem
                                    pmovn:AdvSynItem
                                    pmovn:LexSynItem
                                    pmovn:NpSynItem
                                    pmovn:PrepSynItem
                                    pmovn:VerbSynItem
                                  ) .



###  http://premon.fbk.eu/ontology/vn#SyntacticRestriction

pmovn:SyntacticRestriction rdf:type owl:Class ;
                           
                           rdfs:subClassOf pmovn:Restriction ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty pmo:valueObj ;
                                             owl:allValuesFrom pmovn:SyntacticRestrictionProperty
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty pmo:item ;
                                             owl:allValuesFrom pmovn:SyntacticRestriction
                                           ] .



###  http://premon.fbk.eu/ontology/vn#SyntacticRestrictionProperty

pmovn:SyntacticRestrictionProperty rdf:type owl:Class ;
                                   
                                   rdfs:label "Syntactic Restriction Property" ;
                                   
                                   owl:equivalentClass [ rdf:type owl:Class ;
                                                         owl:oneOf ( pmovn:adv_loc_syrp
                                                                     pmovn:np_omit_ing_syrp
                                                                     pmovn:tensed_that_syrp
                                                                     pmovn:np_ing_syrp
                                                                     pmovn:oc_ing_syrp
                                                                     pmovn:sc_to_inf_syrp
                                                                     pmovn:vc_to_inf_syrp
                                                                     pmovn:small_clause_syrp
                                                                     pmovn:genitive_syrp
                                                                     pmovn:wheth_inf_syrp
                                                                     pmovn:oc_to_inf_syrp
                                                                     pmovn:what_extract_syrp
                                                                     pmovn:oc_bare_inf_syrp
                                                                     pmovn:np_p_ing_syrp
                                                                     pmovn:wh_ing_syrp
                                                                     pmovn:how_extract_syrp
                                                                     pmovn:to_be_syrp
                                                                     pmovn:wh_extract_syrp
                                                                     pmovn:what_inf_syrp
                                                                     pmovn:np_ppart_syrp
                                                                     pmovn:that_comp_syrp
