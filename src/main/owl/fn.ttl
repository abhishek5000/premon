###  http://premon.fbk.eu/ontology/fn#CoreFrameElement

:CoreFrameElement rdf:type owl:Class ;
                  
                  rdfs:label "core frame element"@en ;
                  
                  rdfs:subClassOf :FrameElement .



###  http://premon.fbk.eu/ontology/fn#CoreUnexpressedFrameElement

:CoreUnexpressedFrameElement rdf:type owl:Class ;
                             
                             rdfs:label "core-unexpressed frame element"@en ;
                             
                             rdfs:subClassOf :FrameElement .



###  http://premon.fbk.eu/ontology/fn#Example

:Example rdf:type owl:Class ;
         
         rdfs:subClassOf pmo:Example .



###  http://premon.fbk.eu/ontology/fn#ExtraThematicFrameElement

:ExtraThematicFrameElement rdf:type owl:Class ;
                           
                           rdfs:label "extra-thematic frame element"@en ;
                           
                           rdfs:subClassOf :FrameElement .



###  http://premon.fbk.eu/ontology/fn#FECoreSet

:FECoreSet rdf:type owl:Class ;
           
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty pmo:item ;
                             owl:allValuesFrom :FrameElement
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty pmo:item ;
                             owl:minCardinality "2"^^xsd:nonNegativeInteger
                           ] .



###  http://premon.fbk.eu/ontology/fn#Frame

:Frame rdf:type owl:Class ;
       
       rdfs:label "frame"@en ;
       
       rdfs:subClassOf pmo:Predicate ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty [ owl:inverseOf pmo:evokedConcept
                                        ] ;
                         owl:allValuesFrom :LexicalUnit
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty pmo:semArg ;
                         owl:allValuesFrom :FrameElement
                       ] .



###  http://premon.fbk.eu/ontology/fn#FrameElement

:FrameElement rdf:type owl:Class ;
              
              rdfs:label "frame element"@en ;
              
              rdfs:subClassOf pmo:SemanticArgument ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty [ owl:inverseOf pmo:semArg
                                               ] ;
                                owl:allValuesFrom :Frame
                              ] ;
              
              owl:disjointUnionOf ( :CoreFrameElement
                                    :CoreUnexpressedFrameElement
                                    :ExtraThematicFrameElement
                                    :PeripheralFrameElement
                                  ) .



###  http://premon.fbk.eu/ontology/fn#LUStatus

:LUStatus rdf:type owl:Class ;
          
          rdfs:label "Lexical Unit Status"@en .



###  http://premon.fbk.eu/ontology/fn#LexicalUnit

:LexicalUnit rdf:type owl:Class ;
             
             rdfs:label "lexical unit"@en ;
             
             rdfs:subClassOf pmo:Conceptualization ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty pmo:evokedConcept ;
                               owl:allValuesFrom :Frame
                             ] .



###  http://premon.fbk.eu/ontology/fn#Markable

:Markable rdf:type owl:Class ;
          
          rdfs:subClassOf pmo:Markable .



###  http://premon.fbk.eu/ontology/fn#PeripheralFrameElement

:PeripheralFrameElement rdf:type owl:Class ;
                        
                        rdfs:label "peripheral frame element"@en ;
                        
                        rdfs:subClassOf :FrameElement .



###  http://premon.fbk.eu/ontology/fn#SemType

:SemType rdf:type owl:Class ;
         
         rdfs:label "FrameNet semantic type"@en ;
         
         rdfs:subClassOf pmo:SemanticType .





